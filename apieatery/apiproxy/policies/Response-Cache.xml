    <DisplayName>Response Cache</DisplayName>
    <Properties/>
  <Scope>Exclusive</Scope>
    <CacheKey>
        <Prefix>chefs</Prefix>
        <KeyFragment ref="proxy.pathsuffix" type="string"/>
    </CacheKey>
    <ExpirySettings>
        <TimeoutInSec>30</TimeoutInSec>
    </ExpirySettings>
</ResponseCache>